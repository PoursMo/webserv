{
  "$schema": "./_schema.json",
  "debug": false,
  "servers": [
    {
      "client_max_body_size": 104857600,
      "address": "0.0.0.0",
      "locations": [
        {
          "root": "/app/www",
          "path": "/",
          "method": [
            "GET",
            "POST",
            "DELETE"
          ],
          "autoindex": true,
          "cgi": {
            ".php": "/usr/bin/php-cgi",
            ".py": "/usr/bin/python3.10"
          },
          "upload_store": "/app/upload"
        },
        {
          "root": "/app",
          "path": "/upload"
        }
      ]
    }
  ]
}